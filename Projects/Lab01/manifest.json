{
    "manifest_version": 2,
    "name": "Destiny's Browser",
    "description": "Allows the user to change the look of their Google search page and blocks ads.",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"


    },
    "page_action": { 
        "default_icon": {
            
            "16": "/images/icon16_2.png",
            "48": "/images/icon48_2.png",
            "128": "/images/icon128_2.png"
        }
        
    },

    "content_scripts": [
        {

        "matches": ["https://www.google.com/*"],

        "css": ["main.css"]
    }

]


}